{
	"name": "directus-surveyjs-creator",
	"description": "Directus extension to create surveys using surveyjs/survey-creator",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^11.0.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.2",
		"typescript": "^5.9.3",
		"vue": "^3.5.24"
	},
	"dependencies": {
		"survey-creator-vue": "^2.3.15"
	}
}
